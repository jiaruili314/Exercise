//数组左右和相等的位置

class Solution {
    public int pivotIndex(int[] nums) {
        int sum = 0, leftsum = 0;
        for(int x: nums) sum += x;
        for(int i = 0; i < nums.length; i++){
            if(leftsum == sum - leftsum - nums[i])
                return i;
            else leftsum += nums[i];
        }
        return -1;

    }
}